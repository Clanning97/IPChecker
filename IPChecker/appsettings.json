{
    "Serilog": {
        "Using": [
            "Serilog.Sinks.File"
        ],
        "MinimumLevel": {
            "Default": "Information"
        },
        "Enrich": [
            "FromLogContext"
        ],
        "WriteTo": [
            {
                "Name": "File",
                "Args": {
                    "path": "/var/log/IPChecker.log"
                }
            }
        ]
    }
}